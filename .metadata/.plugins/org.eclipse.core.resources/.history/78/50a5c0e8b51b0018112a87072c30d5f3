	  String name = "Ramy";
	  String username = "ramy@invanto.com";
	  
	  if ((name.equals(obj.myName))) {
			System.out.println("Entered name and DB name value matches");
		} 
		else {
			System.out.println("Record doesn't exists in Database");
		}
	  
	  if ((name.equals(obj.myUsername))) {
			System.out.println("Entered username and DB name value matches");
		} 
		else {
			System.out.println("Record doesn't exists in Database");
		}
  }
  @BeforeClass
  public void beforeClass() {
  }

